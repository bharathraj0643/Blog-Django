{
  "builds": [
    {
      "src": "myapp/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "./build.sh"
      }
    },
    {
      "src":"build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myapp/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles"
}