{
  "builds": [
    {
      "src":"myapp/wsgi.py",
      "use": "@vercel/python",
      "config" : {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src":"build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myapp/wsgi.py"
    }
  ],
  "outputDirectory": "static"
}